{
	"compilerOptions": {
		"noImplicitAny": false,
		"module": "ESNext",
		"target": "ESNext",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"jsx": "react",
		"allowJs": true,
		"checkJs": true,
		"sourceMap": true,
		"noEmit": true
	},
	"include": [
		"js/src/**/*"
	],
	"exclude": [
		"node_modules/", "js/src/**/test/"
	]
}